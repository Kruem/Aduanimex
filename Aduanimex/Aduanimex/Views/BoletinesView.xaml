<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             BindingContext="{Binding Main, Source={StaticResource Locator}}"
             BackgroundColor="{StaticResource BacgroundColorPages}"
             Title="INICIO"
             x:Class="Aduanimex.Views.BoletinesView">
    <ContentPage.Content>
        <StackLayout BindingContext="{Binding Boletines}">
            <WebView x:Name="Webview" Source="{Binding Html}" WidthRequest="1000" HeightRequest="1000" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>